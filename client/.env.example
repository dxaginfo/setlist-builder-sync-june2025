# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000

# Authentication
REACT_APP_JWT_STORAGE_KEY=setlist_builder_token

# Features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_THEME_SWITCHER=true
REACT_APP_ENABLE_OFFLINE_MODE=true

# Performance Mode
REACT_APP_AUTOSCROLL_DEFAULT_SPEED=3
REACT_APP_ENABLE_SETLIST_SHARING=true

# File Upload
REACT_APP_MAX_UPLOAD_SIZE=10
REACT_APP_SUPPORTED_FILE_TYPES=pdf,jpg,png,mp3,txt,docx

# Appearance
REACT_APP_DEFAULT_THEME=light